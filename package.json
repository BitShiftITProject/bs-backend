{
  "name": "bitshift-backend",
  "version": "1.0.0",
  "description": "bitshift backend",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "10.19.0",
    "npm": "6.14.6"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "heroku-postbuild": "npm run build",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t bitshift .",
    "docker:run": "docker run -p 3000:3000 -d bitshift",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.5.0",
    "@loopback/core": "^2.9.4",
    "@loopback/openapi-v3": "^3.4.8",
    "@loopback/repository": "^2.11.1",
    "@loopback/rest": "^6.1.0",
    "@loopback/rest-explorer": "^2.2.9",
    "@loopback/service-proxy": "^2.3.7",
    "aws-sdk": "^2.745.0",
    "buffer": "^5.6.0",
    "dotenv": "^8.2.0",
    "jugglingdb-dynamodb": "^0.1.9-6",
    "loopback-connector-dynamodb": "0.1.0-5",
    "loopback-connector-mongodb": "^5.3.0",
    "loopback-datasource-juggler": "^4.24.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "tslib": "^2.0.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.1",
    "source-map-support": "^0.5.19",
    "@loopback/testlab": "^3.2.3",
    "@types/node": "^10.17.28",
    "@loopback/eslint-config": "^9.0.1",
    "eslint": "^7.7.0",
    "typescript": "~3.9.7"
  }
}
